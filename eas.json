{
	"cli": {
		"version": ">= 14.4.0",
		"appVersionSource": "remote",
		"requireCommit": true
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal"
		},
		"preview": {
			"distribution": "internal",
			"installCommand": "npm install --legacy-peer-deps"
		},
		"production": {
			"autoIncrement": true
		}
	},
	"submit": {
		"production": {}
	}
}
